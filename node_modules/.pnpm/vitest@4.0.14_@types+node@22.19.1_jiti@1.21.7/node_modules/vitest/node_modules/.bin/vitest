#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ijeffro/vortz/packages/frontend/vortz/design/leo/node_modules/.pnpm/vitest@4.0.14_@types+node@22.19.1_jiti@1.21.7/node_modules/vitest/node_modules:/home/ijeffro/vortz/packages/frontend/vortz/design/leo/node_modules/.pnpm/vitest@4.0.14_@types+node@22.19.1_jiti@1.21.7/node_modules:/home/ijeffro/vortz/packages/frontend/vortz/design/leo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ijeffro/vortz/packages/frontend/vortz/design/leo/node_modules/.pnpm/vitest@4.0.14_@types+node@22.19.1_jiti@1.21.7/node_modules/vitest/node_modules:/home/ijeffro/vortz/packages/frontend/vortz/design/leo/node_modules/.pnpm/vitest@4.0.14_@types+node@22.19.1_jiti@1.21.7/node_modules:/home/ijeffro/vortz/packages/frontend/vortz/design/leo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vitest.mjs" "$@"
else
  exec node  "$basedir/../../vitest.mjs" "$@"
fi
