#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ijeffro/vortz/packages/frontend/vortz/design/leo/node_modules/.pnpm/update-browserslist-db@1.1.4_browserslist@4.28.0/node_modules/update-browserslist-db/node_modules:/home/ijeffro/vortz/packages/frontend/vortz/design/leo/node_modules/.pnpm/update-browserslist-db@1.1.4_browserslist@4.28.0/node_modules:/home/ijeffro/vortz/packages/frontend/vortz/design/leo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ijeffro/vortz/packages/frontend/vortz/design/leo/node_modules/.pnpm/update-browserslist-db@1.1.4_browserslist@4.28.0/node_modules/update-browserslist-db/node_modules:/home/ijeffro/vortz/packages/frontend/vortz/design/leo/node_modules/.pnpm/update-browserslist-db@1.1.4_browserslist@4.28.0/node_modules:/home/ijeffro/vortz/packages/frontend/vortz/design/leo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../update-browserslist-db/cli.js" "$@"
else
  exec node  "$basedir/../update-browserslist-db/cli.js" "$@"
fi
